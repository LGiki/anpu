{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "headers": [
        {
            "source": "/llms/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/markdown; charset=utf-8"
                }
            ]
        },
        {
            "source": "/llms.txt",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain; charset=utf-8"
                }
            ]
        }
    ]
}